  "license": "MIT",
  "dependencies": {
    "sha256": "^0.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-plugin-unassert": "^2.1.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.8.0",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.2",
    "power-assert": "^1.4.1"
  }
}
