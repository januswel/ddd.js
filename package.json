{
  "name": "ddd",
  "version": "0.0.0",
  "description": "Eric Evance's DDD library in EcmaScript",
  "main": "index.js",
  "scripts": {
    "build": "babel src -d dist",
    "build:watch": "babel src -d dist -w",
    "test": "mocha --require babel-register",
    "test:watch": "nodemon --watch ./src --watch ./test --exec npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/januswel/ddd.git"
  },
  "keywords": [
    "DDD",
